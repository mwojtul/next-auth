{
  "extends": "@next-auth/tsconfig/tsconfig.adapters.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": ".",
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "strictNullChecks": true,
    "stripInternal": true,
    "declarationMap": true,
    "declaration": true
  },
  "exclude": ["tests", "dist", "jest.config.js", "jest-dynamodb-config.js"]
}
